<mvc:View controllerName="zfioritest0001.controller.mainView" xmlns:mvc="sap.ui.core.mvc" xmlns:t="sap.ui.table" xmlns:core="sap.ui.core" displayBlock="true" xmlns="sap.m">
    <Page id="page" title="{i18n>title}" class="sapUiContentPadding">
        <content>
            <Panel expandable="true" expanded="true" headerText="sap.m.input">
                <sap.ui.layout.form:Form xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" id="__form0">
                    <sap.ui.layout.form:formContainers>
                        <sap.ui.layout.form:FormContainer id="__container5" expandable="true" title="FormContainer">
                            <sap.ui.layout.form:formElements>
                                <sap.ui.layout.form:FormElement label="订单类型" id="__element0">
                                    <sap.ui.layout.form:fields>
                                        <Input width="100%" id="__input0" textFormatMode="KeyValue" liveChange="onChange" showValueHelp="true" valueHelpRequest="onValueHelp" showSuggestion="true" suggestionRows="{/HTvakSet}" suggestionItemSelected=".onSuggestionItemSelected">
                                            <suggestionColumns>
                                                <Column popinDisplay="Inline" demandPopin="true">
                                                    <Label text="订单类型" />
                                                </Column>
                                                <Column popinDisplay="Inline" demandPopin="true">
                                                    <Label text="订单类型描述" />
                                                </Column>
                                            </suggestionColumns>
                                            <suggestionRows>
                                                <ColumnListItem>
                                                    <Label text="{Auart}" />
                                                    <Label text="{Bezei}" />
                                                </ColumnListItem>
                                            </suggestionRows>
                                        </Input>
                                    </sap.ui.layout.form:fields>
                                </sap.ui.layout.form:FormElement>
                                <sap.ui.layout.form:FormElement label="目的港" id="__element1">
                                    <sap.ui.layout.form:fields>
                                        <Input width="100%" id="__input1" textFormatMode="KeyValue" liveChange="onChange" showValueHelp="true" valueHelpRequest="onValueHelp" showSuggestion="true" suggestionRows="{/ZrashMdgSet}" suggestionItemSelected=".onSuggestionItemSelected">
                                            <suggestionColumns>
                                                <Column popinDisplay="Inline" demandPopin="true">
                                                    <Label text="目的港" />
                                                </Column>
                                                <Column popinDisplay="Inline" demandPopin="true">
                                                    <Label text="目的港描述" />
                                                </Column>
                                            </suggestionColumns>
                                            <suggestionRows>
                                                <ColumnListItem>
                                                    <Label text="{Kvgr1}" />
                                                    <Label text="{Bezei}" />
                                                </ColumnListItem>
                                            </suggestionRows>
                                        </Input>
                                    </sap.ui.layout.form:fields>
                                </sap.ui.layout.form:FormElement>
                                <sap.ui.layout.form:FormElement label="起运港" id="__element2">
                                    <sap.ui.layout.form:fields>
                                        <Input width="100%" id="__input2" textFormatMode="KeyValue" liveChange="onChange" showValueHelp="true" valueHelpRequest="onValueHelp" showSuggestion="true" suggestionRows="{/ZrashQygSet}" suggestionItemSelected=".onSuggestionItemSelected">
                                            <suggestionColumns>
                                                <Column popinDisplay="Inline" demandPopin="true">
                                                    <Label text="起运港" />
                                                </Column>
                                                <Column popinDisplay="Inline" demandPopin="true">
                                                    <Label text="起运港描述" />
                                                </Column>
                                            </suggestionColumns>
                                            <suggestionRows>
                                                <ColumnListItem>
                                                    <Label text="{Kvgr2}" />
                                                    <Label text="{Bezei}" />
                                                </ColumnListItem>
                                            </suggestionRows>
                                        </Input>
                                    </sap.ui.layout.form:fields>
                                </sap.ui.layout.form:FormElement>
                                <sap.ui.layout.form:FormElement label="客户编码" id="__element3">
                                    <sap.ui.layout.form:fields>
                                        <Input width="100%" id="__input3" textFormatMode="KeyValue" liveChange="onChange" showValueHelp="true" valueHelpRequest="onValueHelp" showSuggestion="true" suggestionRows="{/ZrashKunnrSet}" suggestionItemSelected=".onSuggestionItemSelected">
                                            <suggestionColumns>
                                                <Column popinDisplay="Inline" demandPopin="true">
                                                    <Label text="客户编码" />
                                                </Column>
                                                <Column popinDisplay="Inline" demandPopin="true">
                                                    <Label text="客户名称" />
                                                </Column>
                                            </suggestionColumns>
                                            <suggestionRows>
                                                <ColumnListItem>
                                                    <Label text="{Kunnr}" />
                                                    <Label text="{Name1}" />
                                                </ColumnListItem>
                                            </suggestionRows>
                                        </Input>
                                    </sap.ui.layout.form:fields>
                                </sap.ui.layout.form:FormElement>
                            </sap.ui.layout.form:formElements>
                        </sap.ui.layout.form:FormContainer>
                        <sap.ui.layout.form:FormContainer id="__container4" />
                    </sap.ui.layout.form:formContainers>
                    <sap.ui.layout.form:layout>
                        <sap.ui.layout.form:ResponsiveGridLayout id="__layout0" labelSpanL="3" labelSpanM="1" emptySpanL="1" emptySpanM="1" columnsL="2" columnsM="1" />
                    </sap.ui.layout.form:layout>
                </sap.ui.layout.form:Form>
            </Panel>
            <Panel expandable="true" expanded="true" headerText="sap.ui.table">
                <t:Table id="uiTable" rows="{/soItem}" selectionMode="MultiToggle" visibleRowCount="6" alternateRowColors="true" enableCellFilter="true" enableColumnFreeze="true" enableGrouping="true" fixedColumnCount="1">
                    <t:extension>
                        <OverflowToolbar>
                            <Button text="查询" icon="sap-icon://search" press="onQuery" />
                            <ToolbarSpacer />
                            <ToolbarSpacer />
                            <Button text="编辑" visible="{/uiTable/display}" icon="sap-icon://edit" type="Transparent" press="onEdit" />
                            <Button text="显示" visible="{/uiTable/edit}" icon="sap-icon://display" type="Transparent" press="onDisplay" />
                            <Button text="导出" icon="sap-icon://excel-attachment" type="Transparent" press="onDataExport" />
                            <Button text="打印" icon="sap-icon://print" type="Transparent" press="onPrint" />
                        </OverflowToolbar>
                    </t:extension>
                    <t:columns>
                        <t:Column filterProperty="Vbeln" sortProperty="Vbeln" autoResizable="true">
                            <Text text="销售订单" />
                            <t:template>
                                <!-- <Input value="{Vbeln}" editable="false" /> -->
                                <Text text="{Vbeln}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column filterProperty="Kunnr" autoResizable="true">
                            <Text text="客户" />
                            <t:template>
                                <Input class="inputLowHigh" value="{Kunnr}" editable="{/uiTable/edit}" />
                            </t:template>
                        </t:Column>
                        <t:Column filterProperty="Auart" autoResizable="true">
                            <Text text="订单类型" />
                            <t:template>
                                <Input class="inputLowHigh" value="{Auart}" editable="{/uiTable/edit}" textFormatMode="KeyValue" showValueHelp="true" valueHelpRequest="onValueHelp" showSuggestion="true" suggestionRows="{/HTvakSet}" suggestionItemSelected=".onSuggestionItemSelected">
                                    <suggestionColumns>
                                        <Column popinDisplay="Inline" demandPopin="true">
                                            <Label text="订单类型" />
                                        </Column>
                                        <Column popinDisplay="Inline" demandPopin="true">
                                            <Label text="订单类型描述" />
                                        </Column>
                                    </suggestionColumns>
                                    <suggestionRows>
                                        <ColumnListItem>
                                            <Label text="{Auart}" />
                                            <Label text="{Bezei}" />
                                        </ColumnListItem>
                                    </suggestionRows>
                                </Input>
                            </t:template>
                        </t:Column>
                        <t:Column filterProperty="Erdat" autoResizable="true">
                            <Text text="创建日期" />
                            <t:template>
                                <Label text="{Erdat}" />
                            </t:template>
                        </t:Column>
                        <t:Column filterProperty="Kunwe_ana" autoResizable="true">
                            <Text text="送达方" />
                            <t:template>
                                <Input class="inputLowHigh" value="{Kunwe_ana}" editable="{/uiTable/edit}" />
                            </t:template>
                        </t:Column>
                    </t:columns>
                </t:Table>
            </Panel>
            <Panel expandable="true" expanded="true" headerText="sap.ui.table">
                <Table id="mTable" growing="true" growingThreshold="8" inset="false">
                    <headerToolbar>
                        <OverflowToolbar id="otbSubheader">
                            <Title text="tableDetails" level="H2" />
                            <ToolbarSpacer />
                            <Button text="编辑" visible="{/mTable/display}" icon="sap-icon://edit" type="Transparent" press="onEditm" />
                            <Button text="显示" visible="{/mTable/edit}" icon="sap-icon://display" type="Transparent" press="onDisplaym" />
                        </OverflowToolbar>
                    </headerToolbar>
                    <columns>
                        <Column width="12em">
                            <Text text="销售订单" />
                        </Column>
                        <Column hAlign="Center">
                            <Text text="客户" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true" hAlign="Center">
                            <Text text="订单类型" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true" hAlign="Center">
                            <Text text="创建日期" />
                        </Column>
                        <Column hAlign="Center">
                            <Text text="送达方" />
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem vAlign="Middle">
                            <cells>
                                <ObjectIdentifier title="{Vbeln}" text="{Zstatus}" />
                                <Text text="{Kunnr}" />
                                <Text text="{Auart}" />
                                <Text text="{Erdat}" />
                                <Text text="{Kunwe_ana}" />
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>
            </Panel>
        </content>
    </Page>
</mvc:View>
